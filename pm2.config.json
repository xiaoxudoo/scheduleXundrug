{
  "name": "express-xundrug-schedule",
  "script": "./index.js",
  "cwd": "./",
  "args": [
    "1"
  ],
  "watch": [
    "./app",
    "./config"
  ],
  "ignore_watch": [
    "node_modules",
    "logs"
  ],
  "watch_options": {
    "followSymlinks": false
  },
  "error_file" : "~/logs/express-xundrug-schedule/app-err.log",
  "out_file"   : "~/logs/express-xundrug-schedule/app-out.log",
  "env": {
    "EGG_SERVER_ENV": "prod",
    "NODE_ENV": "production"
  }
}